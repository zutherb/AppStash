{
  "id": "catalog",
  "kind": "ReplicationController",
  "apiVersion": "v1beta1",
  "desiredState": {
    "replicas": 3,
    "replicaSelector": {"name": "catalog"},
    "podTemplate": {
      "desiredState": {
        "manifest": {
          "version": "v1beta1",
          "id": "catalog",
          "containers": [
            {
              "name": "catalog",
              "image": "zutherb/catalog-frontend",
              "ports": [
                {
                  "containerPort": 80,
                  "hostPort": 80
                }
              ],
              "env": [
                {
                  "name": "CHECKOUT_DESIGN",
                  "value": "standard"
                }
              ]
            }
          ]
        }
      },
      "labels": {
        "name": "catalog"
      }
    }
  },
  "labels": {
    "name": "catalog",
    "role": "frontend"
  }
}